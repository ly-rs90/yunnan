{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals":{
        "webix":true,
        "scheduler":true,
        "__dirname":true,
        "APPNAME":true, "VERSION":true, "PRODUCTION":true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "semi": [
            "error",
            "always"
        ]
    }
}